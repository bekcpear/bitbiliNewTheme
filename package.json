{
  "name": "bitbiliTheme1",
  "version": "1.0.0",
  "description": "A theme made by @cwittlut for pelican",
  "author": "cwittlut",
  "license": "GPLv2",
  "private": true,
  "scripts": {
    "generateTemplates": "rm -rf dist/templates && cp -a src/templates dist/",
    "minifyCSS": "sass -s compressed src/css/common.scss:dist/assets/css/common.css",
    "doWebpack": "webpack -c webpack.config.js",
    "copyWBJS": "cp node_modules/workbox-window/build/workbox-window.prod.mjs* dist/assets/js/",
    "generateSW": "workbox injectManifest workbox-config.js && webpack -c webpack.sw-config.js"
  },
  "dependencies": {
    "lit": "^2.0.2",
    "workbox-background-sync": "^6.3.0",
    "workbox-core": "^6.3.0",
    "workbox-expiration": "^6.3.0",
    "workbox-precaching": "^6.3.0",
    "workbox-routing": "^6.3.0",
    "workbox-strategies": "^6.3.0",
    "workbox-window": "^6.3.0"
  },
  "devDependencies": {
    "sass": "^1.43.3",
    "scss": "^0.2.4",
    "uglify-js": "^3.14.2",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1",
    "workbox-cli": "^6.3.0"
  }
}
